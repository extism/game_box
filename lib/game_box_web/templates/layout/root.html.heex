      </button>
      <div class="collapse navbar-collapse" id="navbarNav">
        <ul class="navbar-nav">
          <li class="nav-item">
            <a class="nav-link" href="/home">Home</a>
          </li>
          <li class="nav-item">
          <.link class="nav-link" href={~p"/dashboard/home"}>Live Dashboard</.link>
          </li>
        </ul>
      </div>
    </div>
  </nav>
    <%= @inner_content %>
